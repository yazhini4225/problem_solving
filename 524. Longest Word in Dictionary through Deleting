class Solution:
    def findLongestWord(self, s: str, dictionary: List[str]) -> str:
        common = []

        for word in dictionary:
            i = j = 0
            while i < len(s) and j < len(word):
                if s[i] == word[j]:
                    j += 1
                i += 1
            if j == len(word):
                common.append(word)
        arr = sorted(common, key=lambda x: (len(x), -ord(x[0])))
        if not arr:
            return ""
        return arr[-1]
